{
  "main": "lib/index.js",
  "name": "@blueflag/proto-blueflag-webpack",
  "version": "0.0.0",
  "description": "An oppinionated webpack config",
  "license": "MIT",
  "author": "Allan Hortle",
  "repository": {
    "type": "git",
    "url": "git@github.com:blueflag/proto-blueflag-webpack.git"
  },
  "bugs": {
    "url": "https://github.com/blueflag/proto-blueflag-webpack/issues"
  },
  "files": [
    "lib"
  ],
  "private": false,
  "scripts": {
    "prepublishOnly": "yarn build",
    "build": "rm -rf lib && NODE_ENV=production babel src --out-dir lib --ignore **/*-test.js",
    "watch": "yarn build -w",
    "flow": "blueflag-test flow",
    "flow-coverage": "blueflag-test flow-coverage -M",
    "lint": "blueflag-test lint",
    "test": "yarn jest",
    "test-all": "yarn lint && yarn flow && yarn test && yarn flow-coverage"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/plugin-proposal-export-default-from": "^7.0.0",
    "babel-plugin-extract-flow-types": "^1.0.0",
    "blueflag-test": "^0.18.1"
  },
  "dependencies": {
    "@babel/core": "^7.1.2",
    "autoprefixer": "^9.1.5",
    "babel-loader": "^8.0.4",
    "babel-preset-blueflag": "^1.0.0",
    "clean-webpack-plugin": "^0.1.19",
    "css-loader": "^1.0.0",
    "file-loader": "^2.0.0",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.4.4",
    "postcss-loader": "^3.0.0",
    "raw-loader": "^0.5.1",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "webpack": "^4.20.2",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.9",
    "webpack-merge": "^4.1.4"
  }
}
