{
  "main": "lib/index.js",
  "files": [
    "lib",
    "flow-typed-shims",
    "jest-preset.js"
  ],
  "bin": {
    "blueflag-test": "lib/index.js"
  },
  "name": "blueflag-test",
  "version": "0.23.0",
  "description": "An oppinionated set of linting and testing tools",
  "license": "MIT",
  "author": "Allan Hortle",
  "repository": {
    "type": "git",
    "url": "git@github.com:blueflag/blueflag-test.git"
  },
  "bugs": {
    "url": "https://github.com/blueflag/blueflag-test/issues"
  },
  "scripts": {
    "build": "rm -rf lib && babel src --out-dir lib --ignore \"**/__tests__/**\" --verbose",
    "prepublishOnly": "yarn run build",
    "watch": "yarn run build -- -w",
    "flow": "./scripts/run flow",
    "lint": "./scripts/run lint",
    "lint-fix": "eslint src --ext js,jsx --fix",
    "test": "yarn jest",
    "test-all": "yarn lint && yarn flow && yarn test"
  },
  "dependencies": {
    "@babel/cli": "^7.6.0",
    "@babel/core": "^7.6.0",
    "@babel/register": "^7.6.0",
    "@babel/runtime": "^7.6.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^24.9.0",
    "babel-preset-blueflag": "^1.0.1",
    "chalk": "^2.4.1",
    "commander": "^2.11.0",
    "dotenv": "^4.0.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "enzyme-matchers": "^7.1.1",
    "eslint": "^4.7.2",
    "eslint-config-blueflag": "^0.10.0",
    "eslint-plugin-flowtype": "^2.36.0",
    "eslint-plugin-jest": "^22.0.0",
    "eslint-plugin-react": "^7.4.0",
    "flow-bin": "^0.108.0",
    "flow-coverage-report": "^0.6.1",
    "fs-extra": "^6.0.0",
    "jest": "^24.9.0",
    "jest-environment-enzyme": "^7.1.1",
    "jest-enzyme": "^7.1.1"
  }
}
